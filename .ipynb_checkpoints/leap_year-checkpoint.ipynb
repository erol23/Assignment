   ],
   "source": [
    "def year(x):\n",
    "    if x % 100 == 0:\n",
    "        if x % 400 == 0:\n",
    "            print(x, \"is a leap year\")\n",
    "        else:\n",
    "            print(x, \"isn't a leap year\")\n",
    "    elif x % 4 == 0:\n",
    "        print(x, \"is a leap year\")\n",
    "    else:\n",
    "        print(x, \"isn't a leap year\")\n",
    "        \n",
    "year(2020)"
   ]
